<Window x:Class="VTSCore.Layers.Maps.StringShowClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Closed="Window_Closed"
        Title="空间地理信息" Height="600" Width="500">
    <Grid>
        <TextBox Name="tbData" Margin="0,0,0,35" IsReadOnly="True" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto"/>
        <Button Name="btSave" Content="导出到文件" Margin="-120,0,0,5" Width="100" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Center" Click="btSave_Click"/>
        <Button Name="btCopy" Content="复制到剪贴板" Margin="120,0,0,5" Width="100" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Center" Click="btCopy_Click"/>
    </Grid>
</Window>
