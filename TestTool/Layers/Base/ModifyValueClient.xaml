<Window x:Class="VTSCore.Layers.Base.ModifyValueClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ResizeMode="NoResize" Closed="Window_Closed"
        Title="参数设置" Height="150" Width="300">
    <Grid>
        <Label Name="lbHeading" Content="值：" Margin="10,10,10,10" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBox Name="tbHeading" Margin="80,10,10,10" Width="180" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Button Name="btSave" Content="确认" Margin="50, 60, 0,0" Width="80" Height="25" Click="btSave_Click" VerticalAlignment="Top" HorizontalAlignment="Left" IsDefault="True"/>
        <Button Name="btQuit" Content="退出" Margin="150, 60, 0,0" Width="80" Height="25" Click="btQuit_Click" VerticalAlignment="Top" HorizontalAlignment="Left"/>
    </Grid>
</Window>
