<Window x:Class="VTSCore.Layers.Plotting.ImportAreaByStringClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ResizeMode="CanResize"
        Title="雷达区域数据导入" Height="300" Width="300">
    <Grid>
        <Label Content="区域名称：" Margin="5" VerticalAlignment="Top"/>
        <TextBox Name="tbName" Margin="85,5,5,35" VerticalAlignment="Top" Height="25"/>
        <Label Content="区域数值：" Margin="5,35,0,0" VerticalAlignment="Top"/>
        <TextBox Name="tbData" Margin="5,60,5,35" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True"/>
        <Button Name="btOk" Content="保存" Click="btOk_Click" Width="100" Height="25" VerticalAlignment="Bottom" Margin="-110,5,5,5" IsDefault="True"/>
        <Button Name="btQuit" Content="取消" Click="btQuit_Click" Width="100" Height="25" VerticalAlignment="Bottom" Margin="110,5,5,5"/>
    </Grid>
</Window>
