<Window x:Class="VTSCore.Layers.Tracks.CCTV.CCTVConfigForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Closed="Window_Closed"
        Title="CCTV设置" Height="200" Width="300" WindowStyle="SingleBorderWindow" ResizeMode="NoResize">
    <Grid>
        <Label Content="视频节点 Ip：" VerticalAlignment="Top" HorizontalAlignment="Left" Margin=" 20,10,0,0"/>
        <TextBox Name="tbIp" VerticalAlignment="Top" HorizontalAlignment="Left" Width="150" Height="25" Margin="110,10,0,0"/>
        <Label Content="网络带宽：" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,50,0,0"/>
        <ComboBox Name="cbCCTVMode" VerticalAlignment="Top" HorizontalAlignment="Left" Width="150" Height="25" Margin="110,50,0,0">
            <ComboBoxItem Content="高" HorizontalAlignment="Stretch"/>
            <ComboBoxItem Content="中"/>
            <ComboBoxItem Content="低"/>
        </ComboBox>
        <Button Name="btOk" Content="确定" Click="btOk_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Width=" 100" Height="25" Margin="30,100,0,0" IsDefault="True"/>
        <Button Name="btQuit" Content="退出" Click="btQuit_Click" VerticalAlignment="Top" HorizontalAlignment="Left" Width=" 100" Height="25" Margin="150,100,0,0"/>
    </Grid>
</Window>
