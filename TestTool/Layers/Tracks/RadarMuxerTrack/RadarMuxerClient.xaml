<Window x:Class="VTSCore.Layers.Tracks.RadarMuxerClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        ResizeMode="NoResize" Closed="Window_Closed"
        Title="雷达融合配置参数" Height="200" Width="300">
    <Grid>
        <Label Content="SubEndpoint:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,10,0,0"/>
        <TextBox Name="tbSubEndpoint" Width="180" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="100,10,0,0"/>
        <Label Content="RpcEndpoint:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5,50,0,0"/>
        <TextBox Name="tbRpcEndpoint" Width="180" Height="25" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="100,50,0,0"/>
        <Button Name="btOk" Content="保存" Width="100" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,90,0,0" Click="btOk_Click" IsDefault="True"/>
        <Button Name="btQuit" Content="取消" Width="100" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="160,90,0,0" Click="btQuit_Click"/>
    </Grid>
</Window>
