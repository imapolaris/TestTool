<Window x:Class="VTSCore.Layers.Tracks.StringSettingClient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ResizeMode="NoResize"
        Title="配置窗口" Height="100" Width="500">
    <Grid>
        <Label Name="lbName" Content="配置:" Margin="5" Height="25" VerticalAlignment="Top"/>
        <TextBox Name="tbData" Margin="60,5,5,35" VerticalAlignment="Top" Height="25"/>
        <Button Name="btOk" Content="保存" Click="btOk_Click" Width="100" Height="25" VerticalAlignment="Bottom" Margin="-110,5,5,5" IsDefault="True"/>
        <Button Name="btQuit" Content="取消" Click="btQuit_Click" Width="100" Height="25" VerticalAlignment="Bottom" Margin="110,5,5,5"/>
    </Grid>
</Window>
